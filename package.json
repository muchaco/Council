  "scripts": {
    "build": "next build",
    "clean": "lsof -ti:3000 2>/dev/null | xargs -r kill 2>/dev/null; pgrep -f '[e]lectron/dist/main.js' 2>/dev/null | xargs -r kill 2>/dev/null; rm -rf out/dev/lock 2>/dev/null; echo 'Cleaned up previous processes'",
    "dev": "next dev",
    "lint": "eslint .",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "electron:build": "tsc -p electron/tsconfig.json",
    "electron:dev": "npm run electron:build && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && NODE_ENV=development electron electron/dist/main.js\"",
    "electron:start": "npm run electron:build && electron electron/dist/main.js",
    "electron:package": "npm run build && npm run electron:build && electron-forge package",
    "electron:make": "npm run build && npm run electron:build && electron-forge make",
    "req": "node scripts/requirements.js",
